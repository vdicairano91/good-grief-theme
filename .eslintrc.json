{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-console": "off",
    "semi": "off",
    "no-cond-assign": "off",
    "quotes": "off",
    "no-redeclare": "off",
    "no-extra-semi": "off",
    "no-mixed-spaces-and-tabs": "off",
    "no-const-assign": "off",
    "no-control-regex": "off"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}